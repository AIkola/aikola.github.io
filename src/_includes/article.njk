---
layout: base.njk
---    
<article class="main-article">
<div class="container container--narrow flow blog-container">
  
    <h1 class="article-title">
      {{title}}
    </h1>
    <figure class="main-article__figure">
      <img src="{{image}}" alt="{{imageAlt}}">
      <figcaption>
        {{imageAlt}}
      </figcaption>
    </figure>
    {{content | safe }}
  
</div>
</article>
